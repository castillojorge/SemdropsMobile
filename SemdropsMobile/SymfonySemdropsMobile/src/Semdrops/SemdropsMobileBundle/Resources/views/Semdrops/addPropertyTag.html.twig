{% extends "SemdropsSemdropsMobileBundle::baseForIPhone.html.twig" %}
{% block header %}Set a Property Tag{% endblock %}
{% block content %}
	<form action="{{ path('donePropertyTag') }}" method="POST" {{form_enctype(form)}}>
	   <br>
	   {{form_label(form.uri,'Here go your uri: ')}} 
	   <br>
	   {{form_widget(form.uri)}}
	   <br>
	   {{form_label(form.propertyTag,'Here go your Property Tag: ')}}
	   <br>
	   {{form_widget(form.propertyTag)}}
	   <br>
	   {{form_label(form.destino,'Here go your Destino: ')}}
	   <br>
	   {{form_widget(form.destino)}}
	   <br>
	   <input type="submit" value ="Add" class= "resetButtonStyle white button" />
	   <input type="reset" value="Clear" class= "resetButtonStyle white button" /><br><br>
	</form>
{% endblock %}
